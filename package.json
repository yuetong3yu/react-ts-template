{
  "name": "react-ts-template",
  "version": "1.0.0",
  "description": "A template for creating React.js applications with TypeScript and Webpack",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config webpack.config.js --env env=development",
    "build": "webpack --config webpack.config.js --env env=production",
    "prettier": "prettier --write './src/**/*.{ts,tsx,js,jsx,css,less,json,md,yml,yaml}'",
    "eslint": "eslint --fix './src/**/*.{ts,tsx,js,jsx}'"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
